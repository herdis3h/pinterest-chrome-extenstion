{
  "name": "Page Redder",
  "action": {},
  "manifest_version": 3,
  "version": "0.1",
  "description": "Turns the page red when you click the icon",
  "permissions": ["activeTab", "scripting", "downloads", "downloads.open"],
  "background": {
    "service_worker": "service-worker.js"
  },
  "host_permissions": [
    "*://*.pinterest.com/*"
  ]
}