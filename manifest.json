{
  "name": "PinSaver",
  "action": {},
  "manifest_version": 3,
  "version": "2.0",
  "version_name": "2.0",
  "description": "PinSaver makes it effortless to save images from Pinterest boards.",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "service-worker.js"
  },
  "host_permissions": [
    "*://*.pinterest.com/*"
  ],
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  }
}