{
  "name": "PinSaver",
  "action": {},
  "manifest_version": 3,
  "version": "1.0",
  "version_name": "1.0 beta",
  "description": "PinSaver makes it effortless to save images from Pinterest boards.",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "service-worker.js"
  },
  "host_permissions": [
    "*://*.pinterest.com/*"
  ]
}